{
  "name": "vue-sx",
  "version": "0.0.0",
  "description": "A plugin for responsive design without media queries for Vue.js 3.x",

  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "live": "live-server --open=Demo.html",
    "dist": "node build/babel.release.js",
    "build:types": "tsc src/index.js -d --declarationDir src/ --declarationMap --emitDeclarationOnly --allowJs"
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidebee/vue-sx.git"
  },
  "keywords": [
    "vuejs",
    "responsive"
  ],
  "author": "James Shen",
  "license": "MIT",
  "dependencies": {
    "json2mq": "^0.2.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.1.4"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
